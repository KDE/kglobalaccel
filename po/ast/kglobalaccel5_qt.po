#: kglobalaccel.cpp:535
msgctxt "KGlobalAccel|"
msgid "Conflict with Global Shortcut"
msgstr "Conflictu col atayu global"

#: kglobalaccel.cpp:536
#, qt-format
msgctxt "KGlobalAccel|"
msgid ""
"The '%1' key combination has already been allocated to the global action "
"\"%2\" in %3.\n"
"Do you want to reassign it from that action to the current one?"
msgstr ""
"La combinación de tecles «%1» yá ta asignada a l'aición global «%2» en %3.\n"
"¿Quies reasignala dende esa aición a l'actual?"

#: kglobalaccel.cpp:545 kglobalaccel.cpp:589
msgctxt "KGlobalAccel|"
msgid "Reassign"
msgstr "Reasignar"

#: kglobalaccel.cpp:567
#, qt-format
msgctxt "KGlobalAccel|"
msgid "The '%1' key combination is registered by application %2 for action %3:"
msgstr ""
"La combinación de tecles «%1» rexistróse pola aplicación %2 pa l'aición %3:"

#: kglobalaccel.cpp:574
#, qt-format
msgctxt "KGlobalAccel|"
msgid "In context '%1' for action '%2'\n"
msgstr "Nel contestu «%1» pa l'aición «%2»\n"

#: kglobalaccel.cpp:578
#, qt-format
msgctxt "KGlobalAccel|"
msgid ""
"The '%1' key combination is registered by application %2.\n"
"%3"
msgstr ""
"La combinación de tecles «%1» rexistróse pola aplicación %2.\n"
"%3"

